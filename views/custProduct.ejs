							</form>
							<!-- products -->
							<table>
								<% if(category.length>0) { %>
									<% if(subcategory.length>0) { %>
										<% if(product.length>0) { %>
											<ul class="products"  >
											<% for(var i=0; i<category.length; i++) { %>
												<% for(var j=0; j<subcategory.length; j++) { %>
													<% for(var k=0; k<product.length; k++) { %>
														<% if(product[k].subcategory_id.equals(subcategory[j]._id)) { %>
															<% if(subcategory[j].category_id.equals(category[i]._id)) { %>
																

																	<li class="product type-product post-2683 status-publish first instock product_cat-accessories has-post-thumbnail shipping-taxable purchasable product-type-simple">
																	<a href="https://jevelin.shufflehound.com/shop1/product/black-glasses/" class="woocommerce-LoopProduct-link woocommerce-loop-product__link">
																	<div class="test">
																		<img width="300" height="300" src='/uplodes/<%= product[k].imagename %>' 
																		class="attachment-woocommerce_thumbnail size-woocommerce_thumbnail" alt=""
																		 srcset="http'/uplodes/<%= product[k].imagename %>'
																			sizes="(max-width: 300px) 100vw, 300px" />
																			</div>
																	
																	</a><div class="sh-woo-post-content-container"><a href="https://jevelin.shufflehound.com/shop1/product/black-glasses/" class="woocommerce-LoopProduct-link woocommerce-loop-product__link">
																		<h2 class="woocommerce-loop-product__title"><%= product[k].pname %></h2>
																		<%= product[k].color %>

																	<div class="additional-information sh-default-color">
																	
																		<%= product[k].warranty %> year warranty <br>
																		
																	</div>
																	<div class="additional-information sh-default-color">
																	
																		<%= product[k].brand_id.brand %><br>
																		
																	</div>
																	
																	<div class="star-rating" role="img" aria-label="Rated 5.00 out of 5"><span style="width:100%">Rated <strong class="rating">5.00</strong> out of 5</span></div>
																	<span class="price"><span class="woocommerce-Price-amount amount"><span class="woocommerce-Price-currencySymbol">Rs </span><%= product[k].price %> /-</span></span>
																	<a href="/customer/<%= product[k]._id %>" style="margin-left: 40px;font-size:20px" class="fa fa-search" ></a>																	</li>
																	
																	
															<% } else { %>
															

															<% } %>
														<% } %>
													<% } %>

												<% } %>
											<% } %>
										</ul>
										<% } %>
									<% }  %>
									<% } else { %>
										
									<%}%>
							</table>
			
						</div>


						<div id="sidebar" class="sidebar-left">

							<div id="woocommerce_product_search-2"
								class="widget-item woocommerce widget_product_search">
								<div class="widget_search">
									<form role="search" method="get" class="search-form"
										action="https://jevelin.shufflehound.com/shop1/">
										<div>
											<label>
												<input type="search" class="sh-sidebar-search search-field"
													placeholder="Search products..." value="" name="s"
													title="Search for:" required />
											</label>
											<button type="submit" class="search-submit">
												<i class="icon-magnifier"></i>
											</button>
										</div>
										<input type="hidden" name="post_type" value="product" />
									</form>
								</div>
							</div>
						
							<%- include('viewcategory'); -%>

						</div>
					</div>


				</div>

			</div>


			<%- include('custFooter'); -%>


		</div>

		<div class="sh-back-to-top sh-back-to-top3">
			<i class="icon-arrow-up"></i>
		</div>

	</div>
	<script>
		function myFunction() {
			var input, filter, ul, li, a, i, txtValue;
			input = document.getElementById("myInput");
			filter = input.value.toUpperCase();
			ul = document.getElementById("myUL");
			li = ul.getElementsByTagName("li");
			for (i = 0; i < li.length; i++) {
				a = li[i].getElementsByTagName("a")[0];
				txtValue = a.textContent || a.innerText;
				if (txtValue.toUpperCase().indexOf(filter) > -1) {
					li[i].style.display = "";
				} else {
					li[i].style.display = "none";
				}
			}
		}
		</script>
		




